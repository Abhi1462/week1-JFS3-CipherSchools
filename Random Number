
import java.util.Random;
import java.util.Scanner;

public class problems {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in); 
        Random rnd = new Random();
        int yourChoice= rnd.nextInt(0, 20);
        System.out.println(" Random Number is " + yourChoice);
        
        for(int i=1; i<=6; i++) {
        System.out.println(" Guess a Number Between 0 to 20");
        int myChoice = sc.nextInt();
         if(myChoice>yourChoice) {
            System.out.println(" You are going far ");
         }
         else if ( myChoice<yourChoice) {
            System.out.println("You are still not there");
         }
         else {
            System.out.println(" Bravo ! You guessed it right");
          break;
         }
        }
 }
}
